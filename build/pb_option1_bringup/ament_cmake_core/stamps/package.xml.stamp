<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>pb_option1_bringup</name>
  <version>0.0.0</version>
  <description>Bringup launch files for pb_option1 robot simulation with real camera</description>
  <maintainer email="2830532983@qq.com">chen</maintainer>
  <license>Apache-2.0</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <!-- 运行时依赖：根据你的 launch 文件实际启动的节点加 -->
  <exec_depend>gazebo_ros</exec_depend>
  <exec_depend>robot_state_publisher</exec_depend>
  <exec_depend>usb_cam</exec_depend>
  <!-- <exec_depend>pb_option1_description</exec_depend>
  <exec_depend>pb_option1_vision</exec_depend> -->

  <!-- 可选：如果你用了 rclpy 或其他 Python 东西，可以加 -->
  <!-- <exec_depend>rclpy</exec_depend> -->

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>